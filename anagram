include <iostream>
#include<string>
#include<algorithm>
using namespace std;
int main() {
    string s;
    cout<<"enter a string"<<endl;
    getline(cin,s);
    
    
    sort(s.begin(),s.end());
    //sort in ascci value so cpital letter not sort if single capiatl letter in string 
    
    string r;
      cout<<"enter a  second string"<<endl;
    getline(cin,r);
    sort(r.begin(),r.end());
    if(s==r)
    {
        cout<<true;
    }
    else { cout<<false; }
    
    

    return 0;
}
