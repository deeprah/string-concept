#include <iostream>
#include<string>
#include<vector>
#include<sstream>
#include<algorithm>
using namespace std;
int main() {
    string str;
    getline(cin,str);
    int i=0;
    for(int i=0;i<str.size();i++)
    {
        if(i%2!=0)
        {
            str[i]='#';
        }
    }
    cout<<str<<endl;
  
   
    return 0;
}
