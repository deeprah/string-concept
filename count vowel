// Online C++ compiler to run C++ program online
#include <iostream>
#include<string>
using namespace std;
int main() {
    string s;
    getline(cin,s);
    cout<<s.size()<<endl;
    int i=0;
    int count=0;
    while(i<s.size())
    {
        if(s[i]=='a' || s[i]=='e' || s[i]=='i'  || s[i]=='o' || s[i]=='u')
        {
            count++;
        }
        i++;
    }
    cout<<count<<endl;
    return 0;
}
